{
  "name": "learnovatex-frontend",
  "buildCommand": "npm run build",
  "outputLocation": "build",
  "routes": [
    {
      "route": "/LearnovateX.mp4",
      "allowedRoles": ["anonymous"],
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/Courses/*",
      "/videos/*",
      "/assets/*",
      "/static/*",
      "/static/media/*",
      "/*.css",
      "/*.js",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.svg",
      "/LearnovateX.mp4"
    ]
  },
  "globalHeaders": {
    "Cache-Control": "no-cache",
    "Referrer-Policy": "strict-origin-when-cross-origin",
    "Permissions-Policy": "accelerometer=*, autoplay=*, clipboard-write=*, encrypted-media=*, gyroscope=*, picture-in-picture=*",
    "X-Content-Type-Options": "nosniff"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".mp4": "video/mp4",
    ".webm": "video/webm"
  },
  "environmentVariables": {
    "REACT_APP_API_BASE_URL": "https://learnovatex-backend-g9cpebe9c9cpfaah.eastasia-01.azurewebsites.net"
  }
}
